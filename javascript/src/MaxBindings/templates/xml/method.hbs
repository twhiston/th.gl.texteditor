{{#if options.paramCount}}
<method name="{{options.functionName}}">
    <arglist>
        {{#each options.params}}
        <arg name="{{this}}" optional="0" type="symbol" />
        {{/each}}
	</arglist>
	<digest>
	    {{#if options.comment}}{{firstSentanceOrLine options.comment}}{{else}}auto-bound to max via annotation{{/if}}
	</digest>
	<description>
        {{#if options.comment}}{{cutComment options.comment}}{{else}}This function was auto bound to the max interface, but it does not have a comment describing what it does.{{/if}}
	</description>
</method>
{{/if}}